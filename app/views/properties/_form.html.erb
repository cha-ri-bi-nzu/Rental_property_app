<%= form_with(model: @picture, local: true) do |f| %>
  <div class="field">
    <p><%= f.label :"物件名" %></p>
    <p><%= f.text_field :name %></p>
    <p><%= f.label :"賃料" %></p>
    <p><%= f.text_field :rete %>円</p>
    <p><%= f.label :"住所" %></p>
    <p><%= f.text_field :address %></p>
    <p><%= f.label :"築年数" %></p>
    <p><%= f.text_field :age %>年</p>
    <p><%= f.label :"備考" %></p>
    <p><%= f.text_area :note %></p>

    <hr width=auto>
    <h2>最寄り駅</h2>
    <p><%= f.label :"路線名" %></p>
    <p><%= f.text_field :route %></p>
    <p><%= f.label :"駅名" %></p>
    <p><%= f.text_field :station %></p>
    <p><%= f.label :"徒歩分数" %></p>
    <p><%= f.text_field :time %>分</p>
  </div>
<% end %>